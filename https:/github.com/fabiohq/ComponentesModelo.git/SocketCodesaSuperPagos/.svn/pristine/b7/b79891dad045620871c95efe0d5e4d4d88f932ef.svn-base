package co.com.codesa.socketcodesaoperadorexterno.bean;

/**
 *
 * @author  Carlos Julio Hernandez 
 */
public class ResultadoBean {
    
    
    private Object objMensaje;
    private Object objResultado;
    
    private boolean boMensaje;
    private boolean boResultado;
    
    private String requiereClave = "";
    private String nroTransaccion = "";
    String loginUsr = "";
        
    /** Creates a new instance of ResultadoBean */
    public ResultadoBean( )
    {
        objMensaje = "";
        objResultado = "";
        
        boMensaje = false;
        boResultado = false;
        
    }
    
    public void setObject( Object obj )
    {
        objResultado = obj;
    }
    
    public void setMensaje( Object obj )
    {
        objMensaje = obj;
    }
    
    public void setResultado( boolean boresultado )
    {
        boResultado = boresultado;
    }
    
    public void conMensaje( boolean bomensaje )
    {
        boMensaje = bomensaje;
    }
    
    public Object getObject( )
    {
        return objResultado;
    }
    
    public boolean getResultado( )
    {
        return boResultado;
    }
    
    public Object getMensaje( )
    {
        return objMensaje;
    }
    
    public boolean getConMensaje( )
    {
        return boMensaje;
    }
    
    

    public String getNroTransaccion() {
        return nroTransaccion;
    }

    public void setNroTransaccion(String nroTransaccion) {
        this.nroTransaccion = nroTransaccion;
    }

    public String getRequiereClave() {
        return requiereClave;
    }

    public void setRequiereClave(String requiereClave) {
        this.requiereClave = requiereClave;
    }

    public String getLoginUsr() {
        return loginUsr;
    }

    public void setLoginUsr(String loginUsr) {
        this.loginUsr = loginUsr;
    }
    
    
    
}
